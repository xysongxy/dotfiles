	"@(": { "prefix": "@1", "body": "\\left( $1 \\right)", "description": "Left and right parentheses" },
	"@{": { "prefix": "@3", "body": "\\left\\{ $1 \\right\\\\}", "description": "Left and right curly braces" },
	"@[": { "prefix": "@4", "body": "\\left[ $1 \\right]", "description": "Left and right brackets" },
	"__": { "prefix": "__", "body": "_{$1}", "description": "Subscript" },
	"**": { "prefix": "**", "body": "^{$1}", "description": "Superscript" },
	"...": { "prefix": "...", "body": "\\dots", "description": "Ellipsis" },
	"@.": { "prefix": "@.", "body": "\\cdot", "description": "Dot product" },
	"@8": { "prefix": "@8", "body": "\\infty", "description": "Infinity symbol" },
	"@6": { "prefix": "@6", "body": "\\partial", "description": "Partial derivative symbol" },
