{
	// Place your snippets for markdown_latex_combined here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"dollar math": {
		//"scope": "tex,latex",
		"prefix": "mm", // this is what you type in the editor to get the snippet suggestion
		"body": ["\\$$1\\$"],
	},
	"Double dollar math": {
		//"scope": "tex,latex",
		"prefix": "mmm", // this is what you type in the editor to get the snippet suggestion
		"body": ["\\$$",
				 "$0",
				 "\\$$"],
	},

	"Equation Environment": {
		"prefix": "BEQ",
		"body": [
			"\\begin{equation}",
			"\t$0",
			"\\end{equation}"
		],
		"description": "Insert equation environment"
	},
	"Starred Equation Environment": {
		"prefix": "BSEQ",
		"body": [
			"\\begin{equation*}",
			"\t$0",
			"\\end{equation*}"
		],
		"description": "Insert starred equation environment"
	},
	"Align Environment": {
		"prefix": "BAL",
		"body": [
			"\\begin{align}",
			"\t$0",
			"\\end{align}"
		],
		"description": "Insert align environment"
	},
	"Starred Align Environment": {
		"prefix": "BSAL",
		"body": [
			"\\begin{align*}",
			"\t$0",
			"\\end{align*}"
		],
		"description": "Insert starred align environment"
	},
	"Itemize Environment": {
		"prefix": "BIT",
		"body": [
			"\\begin{itemize}",
			"\t\\item $0",
			"\\end{itemize}"
		],
		"description": "Insert itemize environment"
	},
	"Enumerate Environment": {
		"prefix": "BEN",
		"body": [
			"\\begin{enumerate}",
			"\t\\item $0",
			"\\end{enumerate}"
		],
		"description": "Insert enumerate environment"
	},
	"Split Environment": {
		"prefix": "BSPL",
		"body": [
			"\\begin{split}",
			"\t$0",
			"\\end{split}"
		],
		"description": "Insert split environment"
	},
	"Cases Environment": {
		"prefix": "BCAS",
		"body": [
			"\\begin{cases}",
			"\t$0",
			"\\end{cases}"
		],
		"description": "Insert cases environment"
	},
	"Frame Environment": {
		"prefix": "BFR",
		"body": [
			"\\begin{frame}{${1:Frame Title}}",
			"\t$0",
			"\\end{frame}"
		],
		"description": "Insert frame environment"
	},
	"Figure Environment": {
		"prefix": "BFI",
		"body": [
			"\\begin{figure}[${1:h}]",
			"\t\\centering",
			"\t\\includegraphics[width=${2:0.8}\\textwidth]{${3:figure}}",
			"\t\\caption{${4:caption}}",
			"\t\\label{fig:${5:label}}",
			"\\end{figure}"
		],
		"description": "Insert figure environment"
	},
	"Part": {
        "prefix": "SPA",
        "body": [
            "\\part{${1:Part Title}}",
            "$0"
        ],
        "description": "Insert part sectioning command"
    },
    "Chapter": {
        "prefix": "SCH",
        "body": [
            "\\chapter{${1:Chapter Title}}",
            "$0"
        ],
        "description": "Insert chapter sectioning command"
    },
    "Section": {
        "prefix": "SSE",
        "body": [
            "\\section{${1:Section Title}}",
            "$0"
        ],
        "description": "Insert section sectioning command"
    },
    "Subsection": {
        "prefix": "SSS",
        "body": [
            "\\subsection{${1:Subsection Title}}",
            "$0"
        ],
        "description": "Insert subsection sectioning command"
    },
    "Subsubsection": {
        "prefix": "SS2",
        "body": [
            "\\subsubsection{${1:Subsubsection Title}}",
            "$0"
        ],
        "description": "Insert subsubsection sectioning command"
    },
    "Paragraph": {
        "prefix": "SPG",
        "body": [
            "\\paragraph{${1:Paragraph Title}}",
            "$0"
        ],
        "description": "Insert paragraph sectioning command"
    },
    "Subparagraph": {
        "prefix": "SSP",
        "body": [
            "\\subparagraph{${1:Subparagraph Title}}",
            "$0"
        ],
        "description": "Insert subparagraph sectioning command"
    },


