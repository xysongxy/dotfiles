{
  "dollar math": {
    "scope": "rmarkdown",
    "prefix": "mm",
    "body": ["\\$$1\\$"]
  },
  "Double dollar math": {
    "scope": "rmarkdown",
    "prefix": "mmm",
    "body": ["\\$$", "$0", "\\$$"]
  },

  "Equation Environment": {
    "scope": "rmarkdown",
    "prefix": "BEQ",
    "body": ["\\begin{equation}", "\t$0", "\\end{equation}"],
    "description": "Insert equation environment"
  },
  "Starred Equation Environment": {
    "scope": "rmarkdown",
    "prefix": "BSEQ",
    "body": ["\\begin{equation*}", "\t$0", "\\end{equation*}"],
    "description": "Insert starred equation environment"
  },
  "Align Environment": {
    "scope": "rmarkdown",
    "prefix": "BAL",
    "body": ["\\begin{align}", "\t$0", "\\end{align}"],
    "description": "Insert align environment"
  },
  "Starred Align Environment": {
    "scope": "rmarkdown",
    "prefix": "BSAL",
    "body": ["\\begin{align*}", "\t$0", "\\end{align*}"],
    "description": "Insert starred align environment"
  },
  "Itemize Environment": {
    "scope": "rmarkdown",
    "prefix": "BIT",
    "body": ["\\begin{itemize}", "\t\\item $0", "\\end{itemize}"],
    "description": "Insert itemize environment"
  },
  "Enumerate Environment": {
    "scope": "rmarkdown",
    "prefix": "BEN",
    "body": ["\\begin{enumerate}", "\t\\item $0", "\\end{enumerate}"],
    "description": "Insert enumerate environment"
  },
  "Split Environment": {
    "scope": "rmarkdown",
    "prefix": "BSPL",
    "body": ["\\begin{split}", "\t$0", "\\end{split}"],
    "description": "Insert split environment"
  },
  "Cases Environment": {
    "scope": "rmarkdown",
    "prefix": "BCAS",
    "body": ["\\begin{cases}", "\t$0", "\\end{cases}"],
    "description": "Insert cases environment"
  },
  "Frame Environment": {
    "scope": "rmarkdown",
    "prefix": "BFR",
    "body": ["\\begin{frame}{${1:Frame Title}}", "\t$0", "\\end{frame}"],
    "description": "Insert frame environment"
  },
  "Figure Environment": {
    "scope": "rmarkdown",
    "prefix": "BFI",
    "body": [
      "\\begin{figure}[${1:h}]",
      "\t\\centering",
      "\t\\includegraphics[width=${2:0.8}\\textwidth]{${3:figure}}",
      "\t\\caption{${4:caption}}",
      "\t\\label{fig:${5:label}}",
      "\\end{figure}"
    ],
    "description": "Insert figure environment"
  },

  "Part": {
    "scope": "rmarkdown",
    "prefix": "SPA",
    "body": ["\\part{${1:Part Title}}", "$0"],
    "description": "Insert part sectioning command"
  },
  "Chapter": {
    "scope": "rmarkdown",
    "prefix": "SCH",
    "body": ["\\chapter{${1:Chapter Title}}", "$0"],
    "description": "Insert chapter sectioning command"
  },
  "Section": {
    "scope": "rmarkdown",
    "prefix": "SSE",
    "body": ["\\section{${1:Section Title}}", "$0"],
    "description": "Insert section sectioning command"
  },
  "Subsection": {
    "scope": "rmarkdown",
    "prefix": "SSS",
    "body": ["\\subsection{${1:Subsection Title}}", "$0"],
    "description": "Insert subsection sectioning command"
  },
  "Subsubsection": {
    "scope": "rmarkdown",
    "prefix": "SS2",
    "body": ["\\subsubsection{${1:Subsubsection Title}}", "$0"],
    "description": "Insert subsubsection sectioning command"
  },
  "Paragraph": {
    "scope": "rmarkdown",
    "prefix": "SPG",
    "body": ["\\paragraph{${1:Paragraph Title}}", "$0"],
    "description": "Insert paragraph sectioning command"
  },
  "Subparagraph": {
    "scope": "rmarkdown",
    "prefix": "SSP",
    "body": ["\\subparagraph{${1:Subparagraph Title}}", "$0"],
    "description": "Insert subparagraph sectioning command"
  },

