  "zlp": { "scope": "rmarkdown", "prefix": "zlp", "body": "\\left( ${1} \\right)" },
  "zlb": { "scope": "rmarkdown", "prefix": "zlb", "body": "\\left[ ${1} \\right]" },
  "zlc": { "scope": "rmarkdown", "prefix": "zlc", "body": "\\\\left\\\\{ ${1} \\\\right\\\\}" },

  "zfr": { "scope": "rmarkdown", "prefix": "zfr", "body": "\\frac{${1}}{${2}}" },
  "zsq": { "scope": "rmarkdown", "prefix": "zsq", "body": "\\sqrt{${1}}" },
  "zint": { "scope": "rmarkdown", "prefix": "zint", "body": "\\int_{${1}}^{${2}}" },

  "zhat": { "scope": "rmarkdown", "prefix": "zhat", "body": "\\hat{${1}}" },
  "zbar": { "scope": "rmarkdown", "prefix": "zbar", "body": "\\bar{${1}}" },
  "ztd": { "scope": "rmarkdown", "prefix": "ztd", "body": "\\tilde{${1}}" },
  "zdot": { "scope": "rmarkdown", "prefix": "zdot", "body": "\\dot{${1}}" },
  "zddot": { "scope": "rmarkdown", "prefix": "zddot", "body": "\\ddot{${1}}" },
  "zck": { "scope": "rmarkdown", "prefix": "zck", "body": "\\check{${1}}" },

  "zcd": { "scope": "rmarkdown", "prefix": "zcd", "body": "\\cdot" },
  "zin": { "scope": "rmarkdown", "prefix": "zin", "body": "\\infty" },
  "zpd": { "scope": "rmarkdown", "prefix": "zpd", "body": "\\partial" },
  "zcirc": { "scope": "rmarkdown", "prefix": "zcirc", "body": "\\circ" },
  "zdeg": { "scope": "rmarkdown", "prefix": "zdeg", "body": "^\\circ" },

  "ztimes": { "scope": "rmarkdown", "prefix": "ztimes", "body": "\\times" },
  "zeq": { "scope": "rmarkdown", "prefix": "zeq", "body": "\\equiv" },
  "zle": { "scope": "rmarkdown", "prefix": "zle", "body": "\\leq" },
  "zge": { "scope": "rmarkdown", "prefix": "zge", "body": "\\geq" },

  "zcup": { "scope": "rmarkdown", "prefix": "zcup", "body": "\\bigcup" },
  "zcap": { "scope": "rmarkdown", "prefix": "zcap", "body": "\\bigcap" },
  "zsm": { "scope": "rmarkdown", "prefix": "zsm", "body": "\\setminus" },
  "zand": { "scope": "rmarkdown", "prefix": "zand", "body": "\\wedge" },
  "znon": { "scope": "rmarkdown", "prefix": "znon", "body": "\\nonumber" },

  "z_": { "scope": "rmarkdown", "prefix": "z_", "body": "_{${1}}" },
  "z^": { "scope": "rmarkdown", "prefix": "z^", "body": "^{${1}}" },
  "zdots": { "scope": "rmarkdown", "prefix": "zdots", "body": "\\dots" },

  "zmm": { "scope": "rmarkdown", "prefix": "zmm", "body": "$${1}$$" },

