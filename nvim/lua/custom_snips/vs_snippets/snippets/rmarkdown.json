  },

  "Font Size Selection": {
    "scope": "rmarkdown",
    "prefix": "fontsize",
    "body": "\\fontsize{${1:10pt}}{${2:12pt}}\\selectfont",
    "description": "Opens font size selection"
  },
  "Text Normal": { "scope": "rmarkdown", "prefix": "FNO", "body": "\\textnormal{${1}}", "description": "Normal text font" },
  "Text Roman": { "scope": "rmarkdown", "prefix": "FRM", "body": "\\textrm{${1}}", "description": "Roman text font" },
  "Emphasized Text": { "scope": "rmarkdown", "prefix": "FEM", "body": "\\emph{${1}}", "description": "Emphasized text" },
  "Sans Serif Text": { "scope": "rmarkdown", "prefix": "FSF", "body": "\\textsf{${1}}", "description": "Sans-serif text" },
  "Typewriter Text": { "scope": "rmarkdown", "prefix": "FTT", "body": "\\texttt{${1}}", "description": "Typewriter text" },
  "Italic Text": { "scope": "rmarkdown", "prefix": "FIT", "body": "\\textit{${1}}", "description": "Italic text" },
  "Slanted Text": { "scope": "rmarkdown", "prefix": "FSL", "body": "\\textsl{${1}}", "description": "Slanted text" },
  "Small Caps Text": { "scope": "rmarkdown", "prefix": "FSC", "body": "\\textsc{${1}}", "description": "Small caps text" },
  "Underlined Text": { "scope": "rmarkdown", "prefix": "FUL", "body": "\\underline{${1}}", "description": "Underlined text" },
  "Uppercase Text": { "scope": "rmarkdown", "prefix": "FUC", "body": "\\uppercase{${1}}", "description": "Uppercase text" },
  "Lowercase Text": { "scope": "rmarkdown", "prefix": "FLC", "body": "\\lowercase{${1}}", "description": "Lowercase text" },
  "Bold Text": { "scope": "rmarkdown", "prefix": "FBF", "body": "\\textbf{${1}}", "description": "Bold text" },
  "Superscript Text": { "scope": "rmarkdown", "prefix": "FSS", "body": "\\textsuperscript{${1}}", "description": "Superscript text" },
  "Subscript Text": { "scope": "rmarkdown", "prefix": "FBS", "body": "\\textsubscript{${1}}", "description": "Subscript text" },

  "Summation": { "scope": "rmarkdown", "prefix": "sum", "body": ["\\sum_{${1:}}^{${3:}}"], "description": "Insert a summation formula" },
  "Product": { "scope": "rmarkdown", "prefix": "prod", "body": ["\\prod_{${1:}}^{${3:}}"], "description": "Insert a product formula" },
  "Integral": { "scope": "rmarkdown", "prefix": "int", "body": ["\\int_{${1:}}^{${2:}}"], "description": "Insert an integral" },
  "Double Integral": { "scope": "rmarkdown", "prefix": "dblint", "body": ["\\iint_{${1:}}"], "description": "Insert a double integral" },
  "Limit": { "scope": "rmarkdown", "prefix": "lim", "body": ["\\lim_{${1:}}"], "description": "Insert a limit" },
  "Derivative": { "scope": "rmarkdown", "prefix": "deriv", "body": ["\\frac{d${1:}}{d${2:}}"], "description": "Insert a derivative" },
  "Partial Derivative": { "scope": "rmarkdown", "prefix": "partial", "body": ["\\frac{\\partial ${1:}}{\\partial ${2:}}"], "description": "Insert a partial derivative" },

  "Insert Current Date": {
    "scope": "rmarkdown",
    "prefix": "date",
    "body": ["${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}"],
    "description": "Insert the current date in YYYY-MM-DD format"
  },

  "Note Template": {
    "scope": "rmarkdown",
    "prefix": "tn",
    "body": [
      "---",
      "author: Xiangyu Song",
      "tags: ",
      "created: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}",
      "modified: ",
      "output:",
      "    pdf_document:",
      "        latex_engine: pdflatex",
      "fontsize: 11pt",
      "header-includes:",
      "    - \\\\usepackage{mathpazo}",
      "    - \\\\usepackage{amsmath}",
      "    - \\\\usepackage{geometry}",
      "    - \\\\geometry{letterpaper, margin=1in}",
      "    - \\\\usepackage{setspace}",
      "    - \\\\doublespacing",
      "---"
    ],
    "description": "Note Template"
  },

